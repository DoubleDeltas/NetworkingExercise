package udpPractice;

import java.net.*;
import java.util.*;

public class UdpSendingPractice {
	public static void main(String[] args) {
		DatagramSocket	udpSocket	= null;
		InetAddress		ipAddress	= InetAddress.getByName("192.168.200.182");
		int				port		= 59595;
		try {
			udpSocket	= new DatagramSocket(port, InetAddress.getByName(""))
		} catch (IOException e) {
			
		} finally {
			udpSocket.close();
		}
	}
}
